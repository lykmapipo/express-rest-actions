{
  "name": "@lykmapipo/express-rest-actions",
  "version": "0.8.23",
  "description": "Opinionated rest actions for express",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd logs",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test mocha --require @babel/register test/**/*.spec.js",
    "docs": "doxdox 'lib/**/*.js' -p package.json -l markdown -o DOCUMENTATION.md",
    "cmt": "git add -A && git-cz",
    "changelog": "changelog"
  },
  "keywords": [
    "lykmapipo",
    "express",
    "router",
    "middleware",
    "http",
    "connect",
    "blueprints",
    "rest",
    "actions",
    "get",
    "post",
    "put",
    "patch",
    "delete",
    "search",
    "stale",
    "fresh"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lykmapipo/express-rest-actions.git"
  },
  "author": {
    "name": "lykmapipo",
    "email": "lallyelias87@gmail.com",
    "url": "https://github.com/lykmapipo"
  },
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lykmapipo/express-rest-actions/issues"
  },
  "homepage": "https://github.com/lykmapipo/express-rest-actions#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "@commitlint/travis-cli": "^9.1.1",
    "@lykmapipo/express-test-helpers": "^0.9.11",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "doxdox": "^3.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "generate-changelog": "^1.8.0",
    "handlebars": "^4.7.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash.template": "^4.5.0",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "shelljs": "^0.8.4"
  },
  "peerDependencies": {},
  "dependencies": {
    "@lykmapipo/common": ">=0.38.1",
    "@lykmapipo/env": ">=0.17.15",
    "@lykmapipo/express-common": ">=0.18.11",
    "is-stream": ">=2.0.0",
    "lodash": ">=4.17.19"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "git add -A"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
